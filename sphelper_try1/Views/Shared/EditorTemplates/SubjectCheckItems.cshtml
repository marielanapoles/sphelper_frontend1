@model sphelper_try1.Models.SubjectCheckItems




<tr>
    <td class="table-semgroup" colspan="3" height="24">Semester @Html.DisplayFor(model => model.Semester)</td>
</tr>
@foreach (var item in @Model.Subjects)
{
    <tr>
        <td> @Html.DisplayFor(model => item.SubjectCode)</td>
        <td>
            <div class="buff3">
                @Html.DisplayFor(model => item.SubjectTitle)
            </div>
            <div class="desc">
                @Html.DisplayFor(model => item.SubjectDescription)
            </div>
        </td>
        <td class="checkboxitem">@Html.CheckBoxFor(model => model.IsChecked, new { @class = "" })</td>
    </tr>
}

