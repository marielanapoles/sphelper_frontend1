@model sphelper_try1.Models.SubjectCheckItems


<tr>
    <td> @Model.SubjectCode </td>
    <td>
        <div>
            @Model.SubjectTitle
        </div>
        <div>
            @Model.SubjectDescription
        </div>
        <div>
            <p>Pre requisite: @Model.Prerequisite</p>
        </div>
    </td>
    <td>@Html.CheckBoxFor(model => model.IsChecked)</td>
</tr>

