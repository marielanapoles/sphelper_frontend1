@model sphelper_try1.Models.TableItems

@*reusable template*@
<tr>
    <td class="table-semgroup" colspan="3">Semester @Html.DisplayFor(model => model.Semester)</td>
</tr>
@foreach (var item in @Model.Subjects)
{
    <tr>
        <td> @Html.DisplayFor(model => item.SubjectCode)</td>
        <td>
            <div>
                @Html.DisplayFor(model => item.SubjectTitle)
            </div>
            <div>
                @Html.DisplayFor(model => item.SubjectDescription)
            </div>
        </td>
        <td>FC</td>
    </tr>
}


